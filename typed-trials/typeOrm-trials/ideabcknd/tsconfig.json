{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "ES2016",
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,
      "esModuleInterop": true
   }
}